  // We are specifying the data in a projected geographic coordinate system.
  cs-data = geo-projected {
    to-meters = 1.0
    ellipsoid = clrk66
    datum-horiz = NAD27
    datum-vert = ellipsoid
    projector = projection {
      projection = utm
      units = m
      proj-options = +zone=11
    }
  }
