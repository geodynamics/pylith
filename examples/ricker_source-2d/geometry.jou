# ----------------------------------------------------------------------
# Create block
# ----------------------------------------------------------------------
# ----------------------------------------------------------------------
# Set units to SI.
# ----------------------------------------------------------------------
${Units('si')}
# ----------------------------------------------------------------------
# Create surface using vertices
# ----------------------------------------------------------------------

# Block is 1000.0m x 1000.0m
# 0 m <= x <= 1000.0 m
# 0 m <= y <= 1000.0 m
reset

${blockLength=10.0*km}
${blockWidth=10.0*km}
${blockHeight=10.0*km}

create vertex 0.0 0.0 0.0
create vertex 0.0 {blockWidth} 0.0
create vertex {blockLength} {blockWidth} 0.0
create vertex {blockLength} 0.0 0.0
create surface vertex 1 2 3 4
delete vertex all



