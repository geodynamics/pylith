# ----------------------------------------------------------------------
# Create block
# ----------------------------------------------------------------------

# ----------------------------------------------------------------------
# Create surface using vertices
# ----------------------------------------------------------------------

# Block is 1000.0m x 1000.0m
# 0 m <= x <= 1000.0 m
# 0 m <= y <= 1000.0 m
reset
create vertex 0.0 0.0 0.0
create vertex 0.0 +1000.0 0.0
create vertex +1000.0 +1000.0 0.0
create vertex +1000.0 0.0 0.0
create surface vertex 1 2 3 4
delete vertex all
