# ----------------------------------------------------------------------
# Create surface using vertices
# ----------------------------------------------------------------------

# Block is 1m x 100m
# 0 m <= x <= 100 m
# 0 m <= y <= 1 m
reset
create vertex 0.0 0.0 0.0
create vertex 0.0 +1.0 0.0
create vertex +100.0 +1.0 0.0
create vertex +100.0 0.0 0.0
create surface vertex 1 2 3 4
delete vertex all

